FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


ddtSchemes
{
    default                                             backward;
}

gradSchemes
{
    default                                         Gauss linear;
}

divSchemes
{
    default                                                 none;
    div(phi,U)                                      Gauss linear;
    div(phi,f)                           Gauss limitedLinear01 1;
    div(phi,C2H5OH)                      Gauss limitedLinear01 1;
    div(phi,O2)                          Gauss limitedLinear01 1;
    div(phi,CO2)                         Gauss limitedLinear01 1;
    div(phi,H2O)                         Gauss limitedLinear01 1;
    div(phi,h)                           Gauss limitedLinear01 1;

    div(phi,Xii)        Gauss multivariateIndependent
    {
        f               limitedLinear01 1;
        c               limitedLinear01 1;
    };

    div(phi,Yi_h)       Gauss multivariateSelection
    {
        O2                limitedLinear01 1;
        CH4               limitedLinear01 1;
        CO2               limitedLinear01 1;
        H2O               limitedLinear01 1;
        CO                limitedLinear01 1;
        H2                limitedLinear01 1;
        N2                limitedLinear01 1;
        C2H5OH            limitedLinear01 1;
        h                 limitedLinear01 1;
    };

    div(((rho*nuEff)*dev2(T(grad(U)))))             Gauss linear;
}

laplacianSchemes
{
    default                              Gauss linear orthogonal;
}

interpolationSchemes
{
    default                                               linear;
}

snGradSchemes
{
    default                                           orthogonal;
}

fluxRequired
{
    default                                                   no;
    p;
}


// ************************************************************************* //
