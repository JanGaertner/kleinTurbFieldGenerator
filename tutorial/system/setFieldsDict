FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //



defaultFieldValues
(
    volVectorFieldValue U (-15 0 0)
);

regions
(
    boxToCell
    {
        min (-0.01728 -0.00144  -0.02);
        max ( 0.01728  0.00144   0.02);
        fieldValues
        (
            volVectorFieldValue U (15 0 0)
        );
    }
);

